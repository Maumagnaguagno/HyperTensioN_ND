(defdomain external (

  (:operator (!near ?pred)
    ; Preconditions
    (
      (call approx ?pred)
    )
    ; Del effects
    nil
    ; Add effects
    nil
  )

  (:operator (!store ?element)
    (
      (call <= (call size) 16)
      (call push ?element)
    )
    nil
    nil
  )

  (:operator (!consume ?element)
    (
      (call print-state)
      (call print consuming ?element)
      ;(call input)
    )
    nil
    (
      (consumed ?element)
    )
  )

  (:method (consume-all)
    recursion
    (
      (call > (call size) 0)
    )
    (
      (!consume (call shift))
      (consume-all)
    )

    base
    nil
    nil
  )

  (:method (consume-list ?l)
    recursion
    (
      (call != ?l ())
      (assign ?head (call shiftl ?l))
    )
    (
      (!consume ?head)
      (consume-list ?l)
    )

    base
    nil
    nil
  )
))