(defdomain external (

  (:operator (!get-next)
    ; Preconditions
    (
      (not (call empty))
    )
    ; Del effects
    nil
    ; Add effects
    (
      (have (call get-next-element))
    )
  )

  (:operator (!near ?pred)
    ; Preconditions
    (
      (call approx ?pred)
    )
    ; Del effects
    nil
    ; Add effects
    nil
  )

  (:method (get-all)
    recursion
    nil
    (
      (!get-next)
      (get-all)
    )

    base
    nil
    nil
  )
))