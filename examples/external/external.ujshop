(defdomain external (

  (:operator (!near ?pred)
    ; Preconditions
    (
      (call approx ?pred)
    )
    ; Del effects
    nil
    ; Add effects
    nil
  )

  (:operator (!store ?element)
    (
      (call <= (call size) 16)
      (call push ?element)
    )
    nil
    nil
  )

  (:operator (!consume ?element)
    nil
    nil
    (
      (consumed ?element)
    )
  )

  (:method (consume-all)
    recursion
    (
      (call > (call size) 0)
    )
    (
      (!consume (call shift))
      (consume-all)
    )

    base
    nil
    nil
  )
))